sqlite> create table usuarios (
(x1...>     id integer primary key,
(x1...>     usuario varchar(30),
(x1...>     clave varchar(30),
(x1...>     nombre varchar(50),
(x1...>     apellido varchar(50),
(x1...>     mail varchar(50)
(x1...> );
